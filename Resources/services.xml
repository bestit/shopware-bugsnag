<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="BestItBugsnag\BugsnagClient" id="best_it_bugsnag.bugsnag_client">
            <argument id="shopware.plugin.cached_config_reader" type="service"/>
        </service>
        <service class="BestItBugsnag\BasicSubscriber" id="best_it_bugsnag.basic_subscriber">
            <argument id="best_it_bugsnag.bugsnag_client" type="service"/>
            <tag name="shopware.event_subscriber"/>
        </service>
    </services>
</container>
